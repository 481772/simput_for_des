forceCoeffs1
{
    type        forceCoeffs;

    libs ( "libforces.so" );

    writeControl   {{{forceCoeffs.writeControl}}};
    timeInterval   {{{forceCoeffs.timeInterval}}};

    log            {{{forceCoeffs.log}}};

    patches     ({{{vector forceCoeffs.patches}}});
    rho         rhoInf;      // Indicates incompressible
    rhoInf      1;           // Redundant for incompressible
    liftDir     ({{{vector forceCoeffs.liftDir}}});
    dragDir     ({{{vector forceCoeffs.dragDir}}});
    CofR        ({{{vector forceCoeffs.CofR}}});
    pitchAxis   ({{{vector forceCoeffs.pitchAxis}}});
    magUInf     {{{forceCoeffs.magUInf}}};
    lRef        {{{forceCoeffs.lRef}}};
    Aref        {{{forceCoeffs.Aref}}};
    /*
    binData
    {
        nBin        20;          // output data into 20 bins
        direction   (1 0 0);     // bin direction
        cumulative  yes;
    }
    */
}


